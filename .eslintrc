{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "allowImportExportEverywhere": true
  },
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "extends": [
    "airbnb",
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}
